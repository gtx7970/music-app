{
  "name": "vue-music-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "server:dev": " cd ./back-end && yarn start:dev",
    "frontend:dev": "cd ./front-end && yarn serve",
    "frontend:react:dev": "cd ./front-end-react && yarn start",
    "dev": "npm-run-all --parallel server:dev frontend:dev",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "front-end-reacr/src/**/*.js": [
      "eslint --fix --ext .js",
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.27.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0"
  }
}
